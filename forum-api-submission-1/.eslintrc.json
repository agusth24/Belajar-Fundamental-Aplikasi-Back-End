{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "no-tabs": 0,
        "require-jsdoc": 0,
        "max-len": 0
    }
}